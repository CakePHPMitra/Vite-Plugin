{
  "name": "CakePHPMitra/vite-plugin",
  "description": "CakePHP plugin to integrate Vite with PHP for modern frontend asset bundling",
  "keywords": [
    "cakephp",
    "vite",
    "plugin"
  ],
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Atul Mahankal",
      "email": "atulmahankal@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "CakePhpViteHelper\\": "src/"
    }
  },
  "extra": {
      "cakephp-plugin": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
